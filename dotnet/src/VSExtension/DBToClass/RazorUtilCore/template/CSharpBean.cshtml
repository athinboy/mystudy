@using System.IO
@using System
@using System.Collections.Generic
@using System.Collections
@inherits RazorEngineCore.RazorEngineTemplateBase<Org.FGQ.CodeGenerate.Model.CSharpBeanModel>
using FreeSql.DataAnnotations;

namespace @Model.FullNamespace
{    
@    [Table(Name = "@Model.CSharpClass.EntityTable.TableName")]
